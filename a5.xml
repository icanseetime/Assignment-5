                <type>Baking soda</type>
                <quantity>
                    <amount>2</amount>
                    <measuringType>teaspoons</measuringType>
                </quantity>
            </ingredient>
            <ingredient>
                <type>Cinnamon</type>
                <quantity>
                    <amount>1</amount>
