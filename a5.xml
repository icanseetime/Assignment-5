            </ingredient>
            <ingredient>
                <type>Grated carrots</type>
                <quantity>
                    <amount>3</amount>
                    <measuringType>cups</measuringType>
                </quantity>
            </ingredient>
            <ingredient>
                <type>Eggs</type>
                <quantity>
                    <amount>4</amount>
                    <measuringType>pieces</measuringType>
                </quantity>
            </ingredient>
        </recipe>
