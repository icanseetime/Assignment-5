                <type>White vinegar</type>
                <quantity>
                    <amount>2</amount>
                    <measuringType>tablespoons</measuringType>
                </quantity>
            </ingredient>
        </recipe>
    </cake>
    <cake>
        <name>Carrot cake</name>
        <serves>10</serves>
        <time>PT30M</time>
        <temperature>160 °C</temperature>
