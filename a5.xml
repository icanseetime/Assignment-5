<?xml version="1.0" encoding="UTF-8"?>
<cookbook xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="a5.xsd">
    <cake>
        <name>Pavlova</name>
        <serves>8</serves>
        <time>PT90M</time>
        <temperature>200 °F</temperature>
        <ovenType>Fan</ovenType>
        <difficulty>Medium</difficulty>
        <specialRemarks>Gluten-free</specialRemarks>
        <recipe>
            <ingredient>
                <type>Egg whites</type>
                <quantity>
                    <amount>4</amount>
                    <measuringType>pieces</measuringType>
                </quantity>
            </ingredient>
            <ingredient>
                <type>Superfine sugar</type>
                <quantity>
                    <amount>200</amount>
                    <measuringType>grams</measuringType>
                </quantity>
            </ingredient>
            <ingredient>
                <type>Vanilla extract</type>
                <quantity>
                    <amount>1</amount>
                    <measuringType>teaspoons</measuringType>
                </quantity>
